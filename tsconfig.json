{
  // "$schema": "https://json.schemastore.org/tsconfig",
  // "_version": "20.1.0",
  // "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "./dist",
    "paths": {
      "@libs/*":["src/libs/*"]
    }
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["./node_modules", "./dist"]
}